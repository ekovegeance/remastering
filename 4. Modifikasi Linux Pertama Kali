  sudo apt-get install nama-aplikasi 
# Menginstall Aplikasi Manual Dari Paket.deb
  letakan aplikasi.deb di folder /tmp
  sudo dpkg -i namapaket.deb
# Menginstall paket.deb sekaligus
  sudo dpkg -i *.deb
 
