# Membersihkan Cache Apt
  apt-get clean
# Membersihan dependecis yang tak terpakai
  apt-get autoremove
# Membersihan Bash History
  rm -rf /tmp/* ~/.bash_history
# Unmount proc sys devpts exit 
  umount /proc
  umount /sys
  umount /dev/pts
  exit
  sudo umount edit/dev
  sudo umount mnt
